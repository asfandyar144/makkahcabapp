{
  "expo": {
    "name": "MakkahCab",
    "slug": "MakkahCab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffc61a",
        "permissions": [
          "INTERNET"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
